<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
<!-- 工作流管理 -->
	<package name="BusinessProcess.manage" namespace="/tgbpm/businessprocess" extends="struts-default">
		<action name="*BusinessProcess" method="{1}" class="tgbpm.businessProcess">
			<result name="success">/views/workflow/businessprocess/main.jsp</result>
		</action>
	</package>
<!-- 任务分配管理 -->	
	<package name="BpmTaskUserTg.manage" namespace="/tgbpm/bpmtaskuser" extends="struts-default">
		<action name="*BpmTaskUser" method="{1}" class="tgbpm.bpmTaskUser">
			<result name="success">/views/workflow/businessprocess/main.jsp</result>
		</action>
	</package>
</struts>
